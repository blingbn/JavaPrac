
public class DoubleFor {

	public static void main(String[] args) {

		
		
		
		/*
		//한줄 별 5개
		for(int i =1; i<=5; i++) {
			for(int j=1; j<=10; j++) {
				System.out.print("*");
				
			}System.out.println();
		}
		*/
		
		//삼각형 별 
		
		for(int i = 1; i <= 5; i++) {
			for(int j = 1; j<=i; j++) {
				System.out.print("*");
			}System.out.println();
		}
		
		
	}

}
