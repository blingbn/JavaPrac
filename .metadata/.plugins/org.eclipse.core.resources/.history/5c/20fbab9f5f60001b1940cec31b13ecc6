
public class While {

	public static void main(String[] args) {


		/*
		//출력
		int a = 5;
		String s = "I can do it.";
		
		while(a--!=0) {
			System.out.println(a+ "-" + s);
		}
		*/
		
		//카운트다운
		
		int i = 10;
		System.out.println("카운트 다운을 시작합니다.");
		while(--i!=0) {
			System.out.println(i);
		}System.out.println("Game Over");
	}

}
