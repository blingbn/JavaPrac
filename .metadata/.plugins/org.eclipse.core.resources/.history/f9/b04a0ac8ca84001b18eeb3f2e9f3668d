package workshop07.mobile;

import java.util.HashMap;
import java.util.Map;

public class MapTest {

	public static void main(String[] args) {

		Map<String, Mobile> map = new HashMap<String, Mobile>();
		double sum=0.0;
		
		map.put("1", new Mobile("LU6800","Optimus 2X", 800000));
		map.put("2", new Mobile("SU6600","Optimus Black", 900000));
		map.put("3", new Mobile("KU5700","Optimus Big", 700000));
		map.put("4", new Mobile("SU7600","Optimus Mach", 950000));
		
		System.out.println(map);
		
	}

}
