package com.test3;
public class Ex06_6 {
	String name;
	int salary;

	@Override
	public String getEmployee() {
		return name + " " + salary;
	}
	public Employee() {	}
	
	public Employee(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
