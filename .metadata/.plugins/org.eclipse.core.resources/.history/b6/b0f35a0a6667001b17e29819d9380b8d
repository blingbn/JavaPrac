import java.util.ArrayList;

public class MakeList {
	
	
	ArrayList<Integer> list = new ArrayList<Integer>();
	public MakeList(){
		list.add(1);
		list.add(3);
		list.add(9);
		list.add(7);
		list.add(5);
		list.add(4);
		list.add(6);
		list.add(19);
		list.add(15);
		list.add(25);
	}
	public void makeArrayList(int size) {
		for(int i = 0; i<size; i++)
			System.out.println(i);
	}
	
	
}
