
class Employee {

	String name;
	int salary;

	public String getEmployee() {
		return name+" "+salary;
	}
	public Employee() {	}
	public Employee(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}
}//ends Employee

class Manager extends Employee{
	String depart;

	@Override
	public String getEmployee() {
		return super.getEmployee()+" " + depart;
	
	}
	public Manager(String name, int salary, String depart) {
		super(name,salary);
		this.depart = depart;
	}
}//ends Manager
