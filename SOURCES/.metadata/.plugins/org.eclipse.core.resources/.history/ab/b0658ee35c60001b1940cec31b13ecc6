
public class DoubleFor {

	public static void main(String[] args) {

		
		
		
		/*
		//한줄 별 5개
		for(int i =1; i<=5; i++) {
			for(int j=1; j<=10; j++) {
				System.out.print("*");
				
			}System.out.println();
		}
		*/
		
		/*
		//삼각형 별 
		
		for(int i = 1; i <= 5; i++) {
			for(int j = 1; j<=i; j++) {
				System.out.print("*");
			}System.out.println();
		}
		*/
		//구구단만들기

		for(int i = 2; i <= 9; i++) {
			System.out.printf("==%d==",i);
			System.out.println();
			for(int j = 1; j<=i; j++ ) {
				System.out.printf(i+"*"+j+"="+i*j);
				System.out.println();
			}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
