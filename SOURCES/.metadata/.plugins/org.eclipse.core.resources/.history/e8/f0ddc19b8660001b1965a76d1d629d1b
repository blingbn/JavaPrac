	class Tv {
		// tv의 속성(멤버변수)
		String color;
		boolean power;
		int channel;

		// tv의 기능(메서드)
		void power() {power = !power;}
		void channelUp() {++channel;}
		void channelDown() {--channel;}
		}

	public class TvTest4 {

		public static void main(String[] args) {
			
			Tv [] TvArr = new Tv[3];
			
			for(int i=0; i<TvArr.length; i++) {
				TvArr[i] = new Tv();
				TvArr[i].channel = i+10;
			}
			System.out.println(TvArr[1]);
			
			
			
		}

	}
